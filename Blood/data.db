# Function to initialize the database and insert sample data
def initialize_and_populate_database():
    # Connect to SQLite database (it will be created if it doesn't exist)
    conn = sqlite3.connect('blood_bank.db')
    cursor = conn.cursor()

    # Create the table if it doesn't exist
    cursor.execute('''CREATE TABLE IF NOT EXISTS donors (
                    donor_id INTEGER PRIMARY KEY,
                    name TEXT,
                    blood_group TEXT,
                    age INTEGER,
                    contact_no TEXT)''')

    # Insert sample donor data
    cursor.executemany("INSERT OR IGNORE INTO donors (donor_id, name, blood_group, age, contact_no) VALUES (?, ?, ?, ?, ?)", donor_data)

    # Commit and close the connection
    conn.commit()
    conn.close()

    print("Database initialized and sample data populated successfully.")

# Run the function to populate the database
if __name__ == "__main__":
    initialize_and_populate_database()
